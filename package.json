{
  "name": "css-rules-matcher",
  "version": "0.0.4",
  "type": "module",
  "description": "A lightweight utility to partially restore the behavior of the deprecated window.getMatchedCSSRules method.",
  "repository": {
    "type": "git",
    "url": "https://github.com/belousovjr/css-rules-matcher.git"
  },
  "homepage": "https://github.com/belousovjr/css-rules-matcher#readme",
  "main": "./dist/css-rules-matcher.cjs",
  "module": "./dist/css-rules-matcher.js",
  "unpkg": "./dist/css-rules-matcher.umd.cjs",
  "exports": {
    ".": {
      "import": "./dist/css-rules-matcher.js",
      "require": "./dist/css-rules-matcher.cjs",
      "default": "./dist/css-rules-matcher.umd.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/node": "^22.15.3",
    "typescript": "~5.7.2",
    "vite": "^6.3.1",
    "vite-plugin-dts": "^4.5.3"
  },
  "keywords": [
    "css",
    "style",
    "styles",
    "dom",
    "match",
    "browser",
    "selector",
    "stylesheets",
    "element",
    "css3",
    "css3"
  ],
  "license": "MIT",
  "dependencies": {
    "csstype": "^3.1.3"
  }
}
